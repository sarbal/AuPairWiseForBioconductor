roc_score <- function(scores,labels)
{
negatives = which(labels == 0, arr.ind=T)
scores[negatives] <- 0
p  = sum(scores)
nL = length(labels)
np = sum(labels)
nn = nL - np
roc  <-  (p/np - (np+1)/2)/nn
return(roc)
}
